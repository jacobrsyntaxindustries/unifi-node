{
  "source": {
    "include": ["./src/", "./README.md"],
    "includePattern": "\\.(js|md)$",
    "exclude": ["node_modules/", "test/"]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "metadata": {
    "title": "UniFi Node API Documentation"
  }
}
